{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:PutLogEvents",
                "logs:CreateLogGroup",
                "logs:CreateLogStream"
            ],
            "Resource": "arn:aws:logs:*:*:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": [
                "arn:aws:s3:::sotnich-learn-dataeng-landing/*",
                "arn:aws:s3:::sotnich-learn-dataeng-landing",
                "arn:aws:s3:::sotnich-learn-dataeng-clean/*",
                "arn:aws:s3:::sotnich-learn-dataeng-clean"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "glue:*"
            ],
            "Resource": "*"
        }
    ]
}
